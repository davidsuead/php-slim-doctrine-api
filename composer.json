{
    "description": "API Rest built on Slim 3 + Doctrine 2.",
    "keywords": ["microframework", "rest", "router", "psr7", "slim", "doctrine"],
    "authors": [
        {
            "name": "David Diniz",
            "email": "diniz.david@gmail.com"
        }
    ],
    "require": {
        "doctrine/orm": "2.7.*",
        "symfony/var-dumper": "5.1.*",
        "slim/slim": "3.12.*",
        "slim/twig-view": "2.5.*",
        "awurth/slim-helpers": "^1.0",
        "awurth/slim-validation": "3.3.*",
        "monolog/monolog": "2.1.*",
        "respect/validation": "1.1.*",
        "slim/csrf": "1.0.*",
        "slim/flash": "0.4.*",
        "symfony/dotenv": "^3.0",
        "symfony/http-foundation": "5.1.*",
        "nategood/httpful": "*",
        "zircote/swagger-php": "3.0.*",
        "runcmf/runtracy": "0.2.*",
        "bryanjhv/slim-session": "3.6.*",
        "php-webdriver/webdriver": "1.8.*",
        "phpseclib/phpseclib": "2.0.*",
        "doctrine/migrations": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "ext-curl": "*",
        "ext-zip": "*",
        "phpunit/phpunit-selenium": "8.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/src"
        }
    },
    "autoload-dev": {
        "psr-7": {
            "Tests\\": "tests/src"
        }
    }
}
